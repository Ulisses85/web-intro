    "dev": "parcel public/index.html",
    "build": "parcel public/index.html",
    "format": "prettier --write \"src/**/*.{html,js,jsx,css,scss}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ulisses85/web-intro.git"
  },
  "keywords": [
    "website",
    "personal",
    "Lukasz",
    "Kuligowski",
    "projects",
    "interests",
    "lifestyle",
    "hobbies",
    "technology"
  ],
  "author": "L B Kuligowski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ulisses85/web-intro/issues"
  },
  "homepage": "https://github.com/Ulisses85/web-intro#readme",
  "dependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "lint-staged": "^10.2.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
